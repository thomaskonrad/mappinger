{
  "name": "Mappinger",
  "version": "0.0.0",
  "description": "An open map with the end-user in mind",
  "main": "app/index.html",
  "license": "ISC",
  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"npm run lite\" ",
